{
  "name": "react-ts-liff",
  "description": "LIFFアプリケーションを開発するプロジェクトのサンプル",
  "version": "2.0.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:webdev-jp-net/react-ts-liff.git"
  },
  "author": "webdev",
  "license": "UNLICENSED",
  "scripts": {
    "start": "vite",
    "dev-build": "rimraf dist && tsc && vite build",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "watch": "vite & onchange '**/*.(scss|vue)' -- stylelint '{{file}}' --fix",
    "lint:js": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{scss,tsx} --ignore-path .gitignore",
    "lint": "pnpm lint:js && pnpm lint:style",
    "scaffold": "scaffdog generate",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{scss,tsx}": "pnpm lint:style",
    "*.{js,ts,tsx}": "pnpm lint:js"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "@fontsource/noto-sans-jp": "^5.0.16",
    "@fontsource/quantico": "^5.0.17",
