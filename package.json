{
  "name": "react-ts-liff",
  "description": "LIFFアプリケーションを開発するプロジェクトのサンプル",
  "version": "1.1.0",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git@github.com:webdev-jp-net/react-ts-liff.git"
  },
  "author": "webdev",
  "license": "UNLICENSED",
  "scripts": {
    "start": "react-scripts start",
    "watch": "react-scripts start & onchange 'src/**/*.(scss|tsx)' -- stylelint '{{file}}' --fix --ignore-path .gitignore",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint --ext .js,.ts,.tsx --ignore-path .gitignore .",
    "lint:style": "stylelint src/**/*.{scss,tsx} --ignore-path .gitignore",
    "lint": "npm run lint:js && npm run lint:style",
    "scaffold": "scaffdog generate",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "lint-staged": {
    "*.{scss,tsx}": "npm run lint:style",
    "*.{js,ts,tsx}": "npm run lint:js"
  },
  "browserslist": [
    "last 2 versions"
  ],
  "dependencies": {
    "html-react-parser": "^3.0.1",
    "liff-type": "^2.1.3",
    "modern-css-reset": "^1.4.0",
    "postcss-scss": "^4.0.4",
    "postcss-syntax": "^0.36.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.8.0",
    "react-scripts": "^5.0.1",
    "redux": "^4.2.0",
    "sass": "^1.52.2",
    "scaffdog": "^2.4.0",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@line/bot-sdk": "^7.5.0",
    "@line/liff": "^2.20.1",
    "@reduxjs/toolkit": "^1.8.2",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@stylelint/postcss-css-in-js": "^0.38.0",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/jest": "^28.1.1",
    "@types/node": "^17.0.40",
    "@types/react": "^18.0.11",
    "@types/react-dom": "^18.0.5",
    "@types/react-redux": "^7.1.24",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-storybook": "^0.5.13",
    "fs": "^0.0.1-security",
    "history": "^5.3.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.0",
    "onchange": "^7.1.0",
    "prettier": "^2.6.2",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "tsconfig-paths-webpack-plugin": "^3.5.2",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  }
}
