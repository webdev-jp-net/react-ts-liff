    "sass": "^1.67.0",
    "scaffdog": "^3.0.0",
    "storybook": "^7.4.3",
    "stylelint": "^14.8.5",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-recess-order": "^3.0.0",
    "stylelint-config-standard": "^25.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2",
    "vite": "^4.4.5"
  }
