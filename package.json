    "react-icons": "^4.11.0",
    "react-player": "^2.13.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "redux": "^4.2.1",
    "redux-persist": "^6.0.0",
    "vite-plugin-pwa": "^0.17.4"
  },
  "devDependencies": {
    "@rtk-query/codegen-openapi": "^1.1.1",
